<Page x:Class="assetnest_wpf.MainPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:assetnest_wpf"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="MainPage">

    <Grid>
        <DockPanel>
            <Menu x:Name="menu_top" DockPanel.Dock="Top" Height="18">
                <MenuItem x:Name="menuHome" Header="_Home" Height="18" Click="menuHome_Click"/>
                <MenuItem x:Name="menuProfile" Header="_Profile" Height="18" Click="menuProfile_Click"/>
            </Menu>
            <StackPanel></StackPanel>
            <Frame x:Name="secondFrame" Content="Frame" Margin="0"/>
        </DockPanel>
    </Grid>
</Page>
